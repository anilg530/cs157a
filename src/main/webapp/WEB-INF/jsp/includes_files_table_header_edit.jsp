<form action="#" id="filehub_group_file_new_folder_form" class="form-horizontal" method="post" accept-charset="utf-8">
    <div class="form-group">
        <input id="filehub_folder_name" name="folder_name" type="text" class="form-control auto-width"
               autocomplete="off" autocorrect="off" spellcheck="false" value=""
               placeholder="new folder name" autofocus required>
        <button class="btn btn-default btn-sm form-submit-btn" type="button"
                onclick="filehub_exit_new_folder_html_ajax();"><i class="fa fa-times"></i> Cancel
        </button>
        <button class="btn btn-default btn-sm margin-right-10 form-submit-btn" type="submit"><i
                class="fa fa-folder"></i> Add Folder
        </button>
    </div>
</form>